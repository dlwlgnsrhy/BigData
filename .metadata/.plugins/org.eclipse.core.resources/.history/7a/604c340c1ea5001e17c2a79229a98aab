package 산술연산자;

public class Ex01산술연산자 {

	public static void main(String[] args) {

		//산술 연산자
		//피연산자와 연산자 로 이루어진 식 
		 //a,b      +-*%/
		//이항 연산자(산술 대입 논리) ,단항 연산자() , 삼항 연산자 
		// / :몫 , % : 나머지
		int num1 = 10;
		double num2 = 7f;
		System.out.println(num1);
		System.out.println(num2);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
